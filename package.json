{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@appliedblockchain/assert-combinators": "5.6.0",
    "@appliedblockchain/ci-quality": "workspace:*",
    "@appliedblockchain/eslint-config": "3.1.1",
    "@appliedblockchain/event-recorder": "1.1.0",
    "@appliedblockchain/strading-tooling": "workspace:*",
    "@jest/globals": "^29.6.1",
    "@prelude/async-generator": "0.6.0",
    "@prelude/progress": "0.0.1",
    "@swc-node/register": "1.5.4",
    "@swc/core": "1.3.55",
    "@tsconfig/node18": "^1.0.3",
    "@types/debug": "4.1.5",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.19",
    "@types/ws": "7.4.0",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "axios": "^1.4.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-sonarjs": "^0.20.0",
    "express": "^4.18.2",
    "inquirer": "^7.3.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-junit": "^16.0.0",
    "jsdom": "^21.1.2",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "p-limit": "^3.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.6",
    "wait-on": "^5.3.0",
    "ws": "^7.5.9",
    "xunit-viewer": "^7.2.0"
  },
  "scripts": {
    "bootstrap": "pnpm install --frozen-lockfile --prefer-offline && pnpm run tsc",
    "bootstrap-clean": "pnpm run tsc:clean && pnpm run bootstrap",
    "lint": "pnpm run -r lint --quiet",
    "reset": "find . -name node_modules -exec rm -rf {} + -prune && pnpm install --frozen-lockfile --prefer-offline && pnpm run tsc:clean && pnpm run tsc",
    "start:db": "./packages/strading-dsx/mssql",
    "start:db:az": "./packages/strading-dsx/mssql-az",
    "start:redis": "./packages/strading-dsx/redis",
    "dev:fe": "scripts/dev-fe",
    "dev": "./scripts/dev",
    "dev:dsx": "./scripts/dev-dsx",
    "dev:admin": "./scripts/dev-admin",
    "kill-all": "kill-port 3000 3001 3002 3003 3004 3005 3006 4001 4010 4030 9229 8022 8031 8032 8033 8034 8035 8036 8080 8443",
    "tsc": "tsc --build",
    "tsc:watch": "tsc --build --watch",
    "tsc:clean": "tsc --build --clean",
    "test": "pnpm run -r --workspace-concurrency=1 test",
    "test:vitest": "pnpm run -r --workspace-concurrency=2 test:vitest",
    "test:parallelism": "pnpm run test $(node ./scripts/split.js)",
    "build:dev": "pnpm run -r build:dev",
    "build:test": "pnpm run -r build:test",
    "patch": "./scripts/version-bump patch",
    "minor": "./scripts/version-bump minor",
    "major": "./scripts/version-bump major",
    "build:storybook:ui": "pnpm --filter @appliedblockchain/strading-ui-components run build:storybook ",
    "build:storybook:dsx": "pnpm --filter @appliedblockchain/strading-react run build:storybook ",
    "dep:check": "./scripts/depcheck",
    "is-master": "node ./scripts/isMasterOrReleaseBranch.js --master-only",
    "is-master-or-release-branch": "node ./scripts/isMasterOrReleaseBranch.js",
    "changelog": "changelog > Changelog.md",
    "changelog-csv": "changelog-jira-csv > Changelog.csv",
    "playw:ci": "pnpm --filter @appliedblockchain/strading-playwright run test:ci",
    "make-migration": "node ./packages/strading-core/make-migration.js",
    "generate-migrations": "node ./packages/strading-core/bin/generate-migrations.js",
    "backup:basic": "pnpm run --filter @appliedblockchain/strading-core backup:basic",
    "report-coverage": "pnpm run --filter @appliedblockchain/strading-coverage report",
    "report-unit-html": "node ./scripts/generateUnitHtmlReport.js",
    "report-summary": "node ./scripts/generateSummaryReport.js",
    "has-github-actions-staging-changes": "./scripts/has-github-actions-staging-changes"
  },
  "version": "0.0.1",
  "pnpm": {
    "overrides": {
      "@babel/core": "^7.22.1",
      "d3-color": "^3.1.0"
    }
  }
}
